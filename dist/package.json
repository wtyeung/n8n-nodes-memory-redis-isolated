{
    "name": "n8n-nodes-memory-redis-isolated",
    "version": "0.1.0",
    "description": "Redis-based chat memory node for n8n AI agents with user isolation for queue mode",
    "license": "MIT",
    "homepage": "https://github.com/wtyeung/n8n-nodes-memory-redis-isolated#readme",
    "keywords": ["n8n-community-node-package"],
    "author": { "name": "Tim Yeung", "email": "tim.yeung@hku.hk" },
    "repository": {
        "type": "git",
        "url": "https://github.com/wtyeung/n8n-nodes-memory-redis-isolated.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": ["dist"],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": ["dist/credentials/RedisMemoryIsolated.credentials.js"],
        "nodes": ["dist/nodes/MemoryRedisIsolated/MemoryRedisIsolated.node.js"],
        "strict": false
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^25.0.3",
        "eslint": "9.32.0",
        "n8n-workflow": "^2.1.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "dependencies": {
        "@langchain/classic": "^1.0.6",
        "@langchain/redis": "^0.1.0",
        "langchain": "^0.3.0",
        "redis": "^4.7.0"
    },
    "peerDependencies": { "n8n-workflow": "*" }
}
